; testprogram Godot3DInterpreter
; and action !

TO "CUBE :N :ANGLE
    REPEAT :N
    [
        REPEAT 4
        [
            FORWARD 30
            LEFT 90
        ]
        LEFT :ANGLE
    ]
END


; Start Mainprogram
PENSIZE 3
CAMERA -50 0 0

REPEAT 20
[
    PENCOLOR RANDOM RANDOM RANDOM
    SLEEP 100
    GO "CUBE 36 10
    SLEEP 100
    CAMERA 5 0 -3
    SLEEP 300
    CLEAN
]

PRINT "[END OF PROGRAM]

